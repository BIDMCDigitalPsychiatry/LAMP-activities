{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "array-type": [true, "generic"],
    "interface-name":  [true, "never-prefix"],
    "member-access": false,
    "member-ordering": false,
    "no-console": false
  }
}
